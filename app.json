{
  "expo": {
    "name": "Rubel Engine",
    "slug": "rubel-engine-project",
    "owner": "mdrubel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "rubelengine",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#080C14"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rubelengine.app"
    },
    "android": {
      "package": "com.rubelengine.app",
      "adaptiveIcon": {
        "backgroundColor": "#080C14",
        "foregroundImage": "./assets/android-icon-foreground.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
